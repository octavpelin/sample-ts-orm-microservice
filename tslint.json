{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb"
    ],
    "jsRules": {
        "no-unused-expression": true
    },
    "rules": {
        "max-line-length": [
            true,
            130
        ],
        "prefer-template": false,
        "interface-name": [true, "never-prefix"],
        "import-name": false,
        "no-console": false,
        "max-classes-per-file": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}